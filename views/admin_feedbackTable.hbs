  <!-- custom CSS -->
  <link href="../css/PAWSter%20Care.css" rel="stylesheet">
    
  <!-- custom js -->
    <script src="../js/PAWSter%20Care.js" type="text/javascript"></script>
    <script src="../js/admin_tables.js" type="text/javascript"></script>

</head>

<body>
  {{> navbar_admin}}
    <script>HighlightTabAdmin("feedback_data")</script><br><br><br>
    
    <h2 style="text-align: center" class="post-title">PAWSter Care Feedback</h2><br>

    
        <table id = "FeedbackTable" class="display" style="width=100%">
             <thead>
                <tr>
                    <td>First Name</td> 
                    <td>Last Name</td> 
                    <td>Email Address</td> 
                    <td>Feedback Text</td> 
                </tr>
             </thead>
             <tbody>
            {{#each feedbacks}}
                <tr data-id="{{_id}}">
                    <td>{{fName}}</td>
                    <td>{{lName}}</td>       
                    <td>{{eMail}}</td>       
                    <td>{{fText}}</td>              
                </tr>
            {{/each}}
            </tbody>
        
        </table> 
    

</body>

